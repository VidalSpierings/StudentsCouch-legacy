<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="@dimen/floatingActionButton_margin_bottom"
        android:clickable="true"
        android:src="@mipmap/baseline_done_white_48"
        app:backgroundTint="#E91E63"
        app:fabSize="normal" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dialog_date_linearLayout_height"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentLeft="true"
        android:layout_above="@+id/fab"
        android:layout_below="@+id/yearTextView"
        android:id="@+id/linearLayout">

    </LinearLayout>

    <TextView
        android:id="@+id/yearTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/dialog_date_yearTextView_margin_top"
        android:text="2000"
        android:textSize="@dimen/dialog_date_yearTextView_textSize" />



</RelativeLayout>